{
  "name": "interface",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,js,mjs}\""
  },
  "devDependencies": {
    "eslint": "8.14.0",
    "prettier": "^2.6.2",
    "turbo": "latest",
    "jest": "^28.0.0",
    "ts-jest": "^27.1.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.32.9"
}
