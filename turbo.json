{
  "pipeline": {
    "validate": {
      "cache": false
    },
    "generate": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".graphclient/**", 
        "typechain"
      ],
      "cache": false
    },
    "build": {
      "outputs": [
        ".next/**",
        "dist/**"
      ],
      "dependsOn": [
        "^build",
        "^generate",
        "generate"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "outputs": [],
      "dependsOn": [
        "^build"
      ]  
    },
    "dev": {
      "dependsOn": [
        "^build",
        "generate"
      ],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "dao#build": {
      "dependsOn": [
        "^build",
        "generate",
        "$REDIS_PATH"
      ],
      "outputs": [".next/**"]
    },
    "root#build": {
      "dependsOn": [
        "^build",
        "$ANALYTICS_URL",
        "$BLOG_URL",
        "$DOCS_URL",
        "$FURO_URL",
        "$LANDING_URL"
      ],
      "outputs": [".next/**"]
    }
  }
}
