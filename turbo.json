{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "generate": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "^build",
        ".graphclient/**",
        ".mesh/**",
        "typechain/**"
      ]
    },
    "build": {
      "outputs": [
        ".next/**",
        "dist/**",
        "typechain/**",
        "artifacts/**",
        "cache/**"
      ],
      "dependsOn": [
        "^build",
        "generate"
      ]
    },
    "root#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**"
      ],
      "env": [
        "ANALYTICS_URL",
        "BLOG_URL",
        "FURO_URL",
        "SWAP_URL",
        "LEGACY_URL",
        "EARN_URL",
        "XSWAP_URL"
      ]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": [
        "^build",
        "generate"
      ],
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "lint": {
      "outputs": [],
      "dependsOn": [
        "^build"
      ]
    },
    "validate": {
      "cache": false
    },
    "export": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "exports.json"
      ],
      "cache": true
    },
    "node": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "deployments/**"
      ]
    },
    "server": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
