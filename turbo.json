{
  "pipeline": {
    "validate": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "generate": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "^build",
        ".graphclient/**"
      ],
      "cache": false
    },
    "export": {
      "outputs": [
        "exports/**"
      ]
    },
    "node": {
      "cache": false,
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "deployments/**"
      ]
    },
    "build": {
      "outputs": [
        ".next/**",
        "dist/**",
        "typechain/**",
        "artifacts/**",
        "cache/**"
      ],
      "dependsOn": [
        "^build",
        "generate"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "outputs": [],
      "dependsOn": [
        "^build"
      ]  
    },
    "dev": {
      "dependsOn": [
        "^build",
        "generate"
      ],
      "cache": false
    },

    "dao#build": {
      "dependsOn": [
        "^build",
        "generate",
        "$REDIS_PATH"
      ],
      "outputs": [".next/**"]
    },
    "root#build": {
      "dependsOn": [
        "^build",
        "$ANALYTICS_URL",
        "$BLOG_URL",
        "$DAO_URL",
        "$DOCS_URL",
        "$FURO_URL",
        "$LANDING_URL",
        "$SWAP_URL",
        "$LEGACY_URL"
      ],
      "outputs": [".next/**"]
    }
  }
}
