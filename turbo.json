{
  "pipeline": {
    "validate": {
      "cache": false
    },
    "generate": {
      "cache": false
    },
    "build": {
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "dependsOn": [
        "^build",
        "^generate"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "outputs": [],
      "dependsOn": [
        "^build"
      ]  
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "root#build": {
      "dependsOn": [
        "^build",
        "$ANALYTICS_URL",
        "$BLOG_URL",
        "$DOCS_URL"
      ],
      "outputs": [".next/**"]
    },
    "analytics#generate": {
      "outputs": [".graphclient/**"]
    },
    "furo#generate": {
      "outputs": [".graphclient/**"]
    }
  }
}
