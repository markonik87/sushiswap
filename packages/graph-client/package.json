{
  "name": "@sushiswap/graph-client",
  "version": "0.0.0",
  "private": true,
  "description": "Sushi Graph Client",
  "keywords": [
    "sushi",
    "graph",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "packages/graph-client"
  },
  "license": "MIT",
  "author": "Matthew Lilley <hello@matthewLilley.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup .graphclient/index.ts --format esm,cjs --dts --external graphql",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup .graphclient/index.ts --format esm,cjs --watch --dts --external graphql",
    "dev:gql": "graphclient serve-dev",
    "generate": "graphclient build",
    "lint": "TIMING=1 eslint . --fix",
    "prepublishOnly": "pnpm build",
    "test": "jest --passWithNoTests",
    "validate": "graphclient validate"
  },
  "jest": {
    "preset": "@sushiswap/jest-config/node"
  },
  "devDependencies": {
    "graphql": "^16.5.0",
    "@graphql-mesh/transform-prefix": "0.11.45",
    "@graphql-mesh/transform-rename": "0.12.46",
    "@graphql-mesh/transform-type-merging": "0.4.0",
    "@graphql-mesh/utils": "0.35.0",
    "@graphql-mesh/runtime": "0.35.0",
    "@graphql-mesh/store": "0.8.9",
    "@graphql-mesh/cross-helpers": "0.1.5",
    "@graphql-mesh/cache-localforage": "0.6.6",
    "@graphql-mesh/graphql": "0.26.0",
    "@graphql-mesh/merger-stitching": "0.15.50",
    "@graphprotocol/client-cli": "0.0.11",
    "@sushiswap/eslint-config": "workspace:*",
    "@sushiswap/jest-config": "workspace:*",
    "@sushiswap/prettier-config": "workspace:*",
    "@sushiswap/typescript-config": "workspace:*",
    "@types/jest": "^27.5.1",
    "@types/numeral": "^2.0.2",
    "jest": "^28.1.0",
    "tsup": "^6.0.1",
    "typescript": "^4.0.0"
  }
}
