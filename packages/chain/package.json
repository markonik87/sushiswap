    "preinstall": "pnpm run generate",
    "lint": "TIMING=1 eslint . --fix",
    "test": "jest --passWithNoTests"
  },
  "jest": {
    "preset": "@sushiswap/config/jest/node"
  },
  "devDependencies": {
    "@sushiswap/config": "workspace:*",
    "@types/jest": "^27.5.1",
    "eslint": "^8.10.0",
    "jest": "^28.1.0",
    "typescript": "^4.6.2"
  }
}