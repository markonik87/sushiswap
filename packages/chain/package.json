{
  "name": "@sushiswap/chain",
  "version": "0.0.0",
  "private": true,
  "description": "Sushi Chain",
  "keywords": [
    "sushi",
    "chain"
  ],
  "license": "MIT",
  "author": "Matthew Lilley <hello@matthewLilley.com>",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
