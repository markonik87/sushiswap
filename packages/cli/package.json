  ],
  "license": "MIT",
  "author": "Matthew Lilley <hello@matthewLilley.com>",
  "main": "./dist/index.js",
  "source": "./index.ts",
  "types": "./dist/index.d.ts",
  "bin": {
    "scli": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w",
    "lint": "TIMING=1 eslint src --fix",
